Windows Registry Editor Version 5.00

[HKEY_CLASSES_ROOT\AutoHotkey.File]
@="AutoHotkey.File"

[HKEY_CLASSES_ROOT\AutoHotkey.File\DefaultIcon]
@="\"{basedir}\\AutoHotkey\\AutoHotkey.exe\",0"

[HKEY_CLASSES_ROOT\AutoHotkey.File\shell]

[HKEY_CLASSES_ROOT\AutoHotkey.File\shell\open]

[HKEY_CLASSES_ROOT\AutoHotkey.File\shell\open\command]
@="\"{basedir}\\AutoHotkey\\AutoHotkey.exe\" \"%1\""



[HKEY_CLASSES_ROOT\.ahk]
@="AutoHotkey.File"
"Content Type"="text/plain"

