Windows Registry Editor Version 5.00

;-------context menu-------
[HKEY_CLASSES_ROOT\*\shell]

[HKEY_CLASSES_ROOT\*\shell\notepad2]
@="”√ Notepad2 ±‡º≠"

[HKEY_CLASSES_ROOT\*\shell\notepad2\command]
@="\"{basedir}\\bin\\Notepad2.exe\" \"%1\""

[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows NT\CurrentVersion\Image File Execution Options\notepad.exe]
"Debugger"="\"{basedir}\\bin\\Notepad2.exe\" /z"


;-------text file------------
[HKEY_CLASSES_ROOT\Text.File]
@="Text.File"

[HKEY_CLASSES_ROOT\Text.File\DefaultIcon]
@="\"{basedir}\\bin\\notepad2.exe\",0"

[HKEY_CLASSES_ROOT\Text.File\shell]

[HKEY_CLASSES_ROOT\Text.File\shell\open]

[HKEY_CLASSES_ROOT\Text.File\shell\open\command]
@="\"{basedir}\\bin\\notepad2.exe\" \"%1\""

