Windows Registry Editor Version 5.00


;-------context menu-------
[HKEY_CLASSES_ROOT\*\shell]

[HKEY_CLASSES_ROOT\*\shell\vim]
@="在新的 gVim 窗口中编辑"

[HKEY_CLASSES_ROOT\*\shell\vim\command]
@="\"{basedir}\\Vim\\vim81\\gvim.exe\" \"%1\""

[HKEY_CLASSES_ROOT\*\shell\vim-remote]
@="在已打开的 gVim 窗口中编辑"

[HKEY_CLASSES_ROOT\*\shell\vim-remote\command]
@="\"{basedir}\\Vim\\vim81\\gvim.exe\" --remote-silent \"%1\""

