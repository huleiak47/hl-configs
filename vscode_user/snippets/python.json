{
  "#!": {
    "prefix": "sb",
    "body": [
      "#!/usr/bin/env python3",
      "# -*- coding:${1:utf-8} -*-",
      "\"\"\"",
      "$2",
      "\"\"\"",
      "$0"
    ],
    "description": "Insert header of python code."
  },
  "file coding": {
    "prefix": "coding",
    "body": ["# -*- coding:${1:utf-8} -*-", "$0"],
    "description": "Insert coding header of python code."
  },
  "future": {
    "prefix": "future",
    "body": [
      "from __future__ import unicode_literals",
      "from __future__ import print_function",
      "from __future__ import division",
      "$0"
    ],
    "description": "enable futrue for python2"
  },
  "open file": {
    "prefix": "open",
    "body": ["open(${1:filename}, \"${2:r}\")$0"],
    "description": "open file"
  },
  "with open file": {
    "prefix": "with",
    "body": ["with open(${1:filename}, \"${2:r}\") as ${3:f}:", "    $0"],
    "description": "with open file"
  }
}
